@model MyMvcCv.Models.Entity.TblSosyalMedya

@{
    ViewBag.Title = "SosyalMedyaSayfaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Sosyal Medya Hesap Düzenleme Sayfası</h2>
<br />
<form method="post">
    @Html.Label("Sosyal Medya Adı")
    <br />
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    <br />
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("İkon")
    <br />
    @Html.TextBoxFor(x => x.ikon, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-success">Güncelle</button>
</form>
