@using MyMvcCv.Models.Entity
@model List<TblAdmin>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Admin Kullanıcı Listesi </h2>
<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>Kullanıcı Adı</th>
        <th>Düzenle</th>
        <th>Kaldır</th>
    </tr>
    @foreach (var item in Model)
    {
 
        <tr>
            <td>@item.ID</td>
            <td>@item.KullaniciAdi</td>
            
            <td><a href="/Admin/AdminDuzenle/@item.ID" class="btn btn-danger">Düzenle</a></td>
            <td><a href="/Admin/AdminSil/@item.ID" class="btn btn-success" onclick="return confirm('Silmek istediğinize emin misiniz?');">Sil</a></td>
        </tr>
    }
</table>
<a href="/Admin/AdminEkle" class="btn btn-primary">Yeni Kullanıcı Ekle</a>


